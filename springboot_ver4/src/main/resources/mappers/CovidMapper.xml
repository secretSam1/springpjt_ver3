<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springpjt_ver4.mapper.CovidMapper">

    <select id="getUserDetail" resultType="String" parameterType="hashmap">
	
		SELECT USERID FROM USERINFOT
		WHERE 1=1 
		AND USERID = #{USERID}

    </select>
    
</mapper>